node{
    stage('checkout'){
        git branch: 'main', credentialsId: 'c947e425-c62d-4ca8-8ccf-58bbbc75baa0', url: 'https://github.com/Akshay-Baratagi/maven-web-application.git'
    }
    stage('Build'){
        def mavenHome = tool name: 'mvn 3.9.9', type: 'maven'
        sh "${mavenHome}/bin/mvn clean package"
    }
    stage('sonar analysis'){
        //   def mavenHome = tool name: 'mvn 3.9.9', type: 'maven'
        //   sh "${mavenHome}/bin/ sonar:sonar"
          echo "sonar exected successfully"
    }
    stage ('publish the war file to nexus'){
        echo "nexus configured succesfully"
    }
    stage('deployment'){
        echo "deployed successsfully"
    }
}
